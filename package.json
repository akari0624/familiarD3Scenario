{
  "name": "react_boilerplate_simple_starter",
  "version": "1.0.0",
  "description": "react+redux boilerplate simple starter",
  "main": "index.js",
  "repository": "https://github.com/akari0624/react-starter_withTypeScript",
  "scripts": {
    "dev": "webpack-dev-server --public-host --config ./webpack_config/webpack.config.dev.js --mode development ",
    "devWithBabel": "NODE_ENV=withBabel webpack-dev-server --public-host --config ./webpack_config/webpack.config.dev.js --mode development ",
    "fastbuild_dev": "webpack-dev-server --public-host --config ./webpack_config/webpack.config.dev.fastbuild.js --mode development ",
    "fastbuild_devWithBabel": "NODE_ENV=withBabel webpack-dev-server --public-host --config ./webpack_config/webpack.config.dev.fastbuild.js --mode development ",
    "dist": "npm run clean && webpack --config ./webpack_config/webpack.config.prod.js --progress --bail --env dist -p",
    "distWithBabel": "NODE_ENV=withBabel npm run clean && webpack --config ./webpack_config/webpack.config.prod.js --progress --bail --env dist -p",
    "deploy": "npm run dist && gh-pages -d dist",
    "deployWithBabel": "npm run distWithBabel && gh-pages -d dist",
    "test": "mocha --compilers js:babel-core/register --require babel-polyfill ./test/test_helper.js --recursive ./test",
    "test_ts": "--require ts-node/register './test/**/*.{ts, tsx}'",
    "test:watch": "npm run test -- --watch",
    "clean": "rimraf dist/*"
  },
  "author": "",
  "license": "ISC",
